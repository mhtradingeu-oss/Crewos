{
  "$schema": "https://turbo.build/schema.json",

  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },

    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },

    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },

    "lint": {
      "outputs": []
    },

    "clean": {
      "cache": false
    }
  }
}
